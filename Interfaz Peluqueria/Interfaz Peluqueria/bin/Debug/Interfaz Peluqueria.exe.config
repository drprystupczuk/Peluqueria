      connectionString="Provider=Microsoft.ACE.OLEDB.12.0;Data Source=|DataDirectory|\bin\Debug\Peluqueria.accdb;Persist Security Info=True;OLE DB Services=-13"
      providerName="System.Data.OleDb" />
